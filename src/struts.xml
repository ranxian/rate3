<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <constant name="struts.devMode" value="true" />
    <package name="rate.view" extends="struts-default">
        <action name="user-detail"
                class="rate.view.user.DetailAction"
                method="execute">
            <result name="success">/user-detail.jsp</result>
        </action>
        <action name="user-add"
                class="rate.view.user.AddAction"
                method="execute">
            <result name="fail">/user-add.jsp</result>
            <result name="success">/user-detail.jsp</result>
        </action>
        <action name="user-list"
                class="rate.view.user.ListAction"
                method="execute">
            <result name="success">/user-list.jsp</result>
        </action>
    </package>
</struts>